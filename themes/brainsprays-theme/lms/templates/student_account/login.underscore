<div id="stop" class="scrollTop"></div>
	
<script>
		$(function() {
			var header = $(".menuBar");

			$(window).scroll(function() {    
				var scroll = $(window).scrollTop();
				if (scroll >= 50) {
					header.addClass("scrolled");
				} else {
					header.removeClass("scrolled");
				}
			});

		});
</script>	
	
